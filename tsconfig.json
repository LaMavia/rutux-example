{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"declaration": true,
	},
  "exclude": [
    "./node_modules"
  ]
} 
